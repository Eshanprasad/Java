package practice;
import java.util.Scanner;
public class EshanClass {
	public static int search(int[] arr, int s)
	{
		boolean f = false;
		for(int i=0; i<arr.length; i++)
		{
			if(arr[i] == s)
			{
				return i;
			}
		}
		return -1;      //should return -1, if number is not found in the array
	}

	public static void main(String[] args)
	{
		int[] esh = {1, 2, 3, 15, 18};
		System.out.println(search(esh, 40));
	}
}
