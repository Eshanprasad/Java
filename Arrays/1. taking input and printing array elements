package practice;
import java.util.Scanner;
public class EshanClass {

	public static void main(String[] args)
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int arr[] = new int[n];
		for(int i = 0; i<n;i++)
		{
			int inp = s.nextInt();
			arr[i] = inp;
		}
		for(int j=0; j<n;j++)
		{
			System.out.println("array element "+ j +" =" + arr[j]);
		}
	}
}
