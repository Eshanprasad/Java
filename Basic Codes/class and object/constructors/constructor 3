// here we create two constructors in same class

class Calc
{
	int num1;
	int num2;
	int result;
	
	public Calc()
	{
		num1 = 5;
		num2 = 5;
		System.out.println("In constructor");
	}
	
	public Calc(int n)
	{
	num1 = n;
	num2 = n;
	}
}
public class FirstCode
{
	
	public static void main(String[] args)
	{
		Calc obj = new Calc(7);        // here the constructor is getting called and it is assigning the value 5 to num 1 and value 5 to num 2 
		System.out.println(obj.num1);
	}	
}




/*
Output: 7
*/


-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

class Calc
{
	int num1;
	int num2;
	int result;
	
	public Calc()
	{
		num1 = 5;
		num2 = 5;
		System.out.println("In constructor");
	}
	
	public Calc(int n)
	{
	num1 = n;
	num2 = n;
	}
	
	public Calc(double d, int k)
	{
		num1 = (int)d;
		num2 = k;
	}
}
public class FirstCode
{
	
	public static void main(String[] args)
	{
		Calc obj = new Calc(9.4, 8);        // here the constructor is getting called and it is assigning the value 5 to num 1 and value 5 to num 2 
		System.out.println(obj.num1);
		System.out.println(obj.num2);
	}	
}



/*
output:
9
8
*/


