package practice;
import java.util.Scanner;
public class EshanClass {
	public static boolean isprime(int n){
		if(n==0 || n==1){
			return true;
		}
		else{	
			for(int i=2; i<n; i++){
				if(n%i==0) {
					return false;
				}
			}
		}
		return true;
	}

	public static void main(String[] args)
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		System.out.println(isprime(n));
	}
}
